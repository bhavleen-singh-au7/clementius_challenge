{
  "name": "Assignment",
  "version": "1.0.0",
  "description": "Test Assignment",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node dist/server",
    "test": "nodemon --exec babel-node api/server",
    "client": "npm start --prefix client",
    "build": "rimraf dist/ && babel api/ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "dev": "concurrently \"npm run test\" \"npm run client\""
  },
  "author": "Sahil",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "mongoose": "^5.10.2",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.2",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4"
  }
}
